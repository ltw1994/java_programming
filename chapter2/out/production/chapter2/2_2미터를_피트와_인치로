import java.util.Scanner;

public class Main {
    public static void main(String[] args)
    {
        double M, F, I ;
        Scanner scan = new Scanner(System.in);
        M = scan.nextDouble() ;

        I = M * 39.3701 ;
        F = I / 12 ;

        System.out.println((int)Math.floor(F));
        F -= Math.floor(F) ;
        I = F * 12 ;


        System.out.println((int)I) ;
    }
}